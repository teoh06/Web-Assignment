@model IEnumerable<WebApplication2.Models.Report>
@{
    ViewData["Title"] = "Report History";
}
<h1>Report History</h1>
@if (!Model.Any())
{
    <p>You have not submitted any reports yet.</p>
}
else
{
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Title</th>
                <th>Description</th>
                <th>Status</th>
                <th>Priority</th>
                <th>Created</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var report in Model)
        {
            <tr>
                <td>@report.Title</td>
                <td>@report.Description</td>
                <td>@report.Status</td>
                <td>@report.Priority</td>
                <td>@report.CreatedAt.ToString("g")</td>
            </tr>
        }
        </tbody>
    </table>
}